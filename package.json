{
	"name": "moodle-scraper",
	"version": "0.1.0",
	"prisma": {
		"schema": "src/utils/schema.prisma"
	},
	"scripts": {
		"next:dev": "next dev",
		"tw:dev": "npx tailwindcss -i ./src/styles/tw.css -o ./src/styles/globals.css --watch",
		"dev": "run-p tw:dev next:dev",
		"build": "npx tailwindcss -i ./src/styles/tw.css -o ./src/styles/globals.css --minify && next build",
		"fmt": "npx prettier --write src",
		"start": "next start -p 4500",
		"prod": "pnpm run build && pnpm run start",
		"lint": "next lint",
		"init": "pnpm install && npx prisma db push && npx prisma generate"
	},
	"dependencies": {
		"@next/font": "^13.0.2",
		"@notionhq/client": "^2.2.2",
		"@prisma/client": "^4.5.0",
		"next": "13.0.2",
		"node-cron": "^3.0.2",
		"node-fetch": "^3.2.10",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hot-toast": "^2.4.0",
		"sqlite": "^4.1.2",
		"sqlite3": "^5.1.2",
		"tabler-icons-react": "^1.55.0",
		"web-push": "^3.5.0"
	},
	"devDependencies": {
		"@types/node": "17.0.38",
		"@types/node-cron": "^3.0.5",
		"@types/react": "18.0.25",
		"@types/react-dom": "18.0.8",
		"@types/web-push": "^3.3.2",
		"eslint": "8.16.0",
		"eslint-config-next": "12.1.6",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prisma": "^4.5.0",
		"tailwindcss": "^3.2.2",
		"typescript": "4.7.2"
	}
}
