{
	"name": "moodle-scraper",
	"version": "0.1.0",
	"prisma": {
		"schema": "src/utils/schema.prisma"
	},
	"scripts": {
		"next:dev": "next dev",
		"tw:dev": "npx tailwindcss -i ./src/styles/tw.css -o ./src/styles/globals.css --watch",
		"dev": "run-p tw:dev next:dev",
		"build": "npx tailwindcss -i ./src/styles/tw.css -o ./src/styles/globals.css --minify && next build",
		"fmt": "npx prettier --write src",
		"start": "next start -p 4500",
		"prod": "pnpm run build && pnpm run start",
		"lint": "next lint",
		"init": "pnpm install && npx prisma db push && npx prisma generate"
	},
	"dependencies": {
		"@next/font": "^13.1.6",
		"@prisma/client": "^4.9.0",
		"next": "13.1.6",
		"node-cron": "^3.0.2",
		"node-fetch": "^3.3.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hot-toast": "^2.4.0",
		"sqlite": "^4.1.2",
		"sqlite3": "^5.1.4",
		"tabler-icons-react": "^1.56.0",
		"web-push": "^3.5.0"
	},
	"devDependencies": {
		"@types/node": "18.11.18",
		"@types/node-cron": "^3.0.7",
		"@types/react": "18.0.27",
		"@types/react-dom": "18.0.10",
		"@types/web-push": "^3.3.2",
		"eslint": "8.33.0",
		"eslint-config-next": "13.1.6",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"prisma": "^4.9.0",
		"tailwindcss": "^3.2.4",
		"typescript": "4.9.5"
	}
}
