datasource db {
  provider = "sqlite"
  url      = "file:assignments.db"
}

generator client {
  provider = "prisma-client-js"
}

model assignments {
  id       Int    @id
  name     String
  due      Int
  modified Int
  course   Int
}

model notifications {
  endpoint String @id
  p256dh   String
  auth     String
}

model files {
  id       Int    @id
  name     String
  topic    String
  course   Int
  pages    Int
  modified Int
}
